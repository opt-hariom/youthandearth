<script
  type="application/json"
  data-section-type="custom-liquid"
  data-section-id="{{ section.id }}"
>
</script>


{% assign width = section.settings.width %}

<section
  class="
    section
    {{ section.settings.css_class }}
    is-width-{{ section.settings.width }}
  "
>
  <div class="container">
    
    <div class="one-whole column">
      {{ section.settings.custom_liquid }}
    </div>
  </div>
  </section>
{% schema %}
  {
    "name": "Custom liquid",
    "class": "shopify-section--custom-liquid",
    "settings": [
      {
        "type": "liquid",
        "id": "custom_liquid",
        "label": "Custom liquid"
      },
          {
      "type": "select",
      "id": "width",
      "label": "Width",
      "default": "wide",
      "options": [
        {
          "value": "standard",
          "label": "Standard"
        },
        {
          "value": "wide",
          "label": "Wide"
        }
      ]
    }
    ],
    "presets": [
      {
        "name": "Custom liquid",
        "category": "Content"
      }
    ],
    "disabled_on": {
      "groups": [
        "*"
      ]
    }
  }
{% endschema %}
